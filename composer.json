{
    "name": "beastbytes/postal-code-data-php",
    "type": "library",
    "description": "Implementation of postal-code-data using PHP as the storage",
    "keywords": [
        "postal code"
    ],
    "support": {
        "issues": "https://github.com/beastbytes/postal-code-data-php/issues",
        "source": "https://github.com/beastbytes/postal-code-data-php"
    },
    "license": "BSD-3-Clause",
    "authors": [{
        "name": "Chris Yates",
        "email": "chris.l.yates@gmail.com"
    }],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "beastbytes/postal-code-data": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "consolidation/robo": "^4.0",
        "roave/infection-static-analysis-plugin": "^1.0",
        "roave/security-advisories": "dev-latest",
        "vimeo/psalm": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "BeastBytes\\PostalCode\\PHP\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BeastBytes\\PostalCode\\PHP\\Tests\\": "tests"
        }
    },
    "scripts": {
        "infection": "roave-infection-static-analysis-plugin",
        "psalm": "psalm",
        "robo": "robo",
        "test": "phpunit --testdox"
    }
}
